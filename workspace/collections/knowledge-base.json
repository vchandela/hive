{
  "name": "knowledge-base",
  "documents_dir": "workspace/documents",
  "chunking": {
    "heading_level": 2,
    "max_tokens": 512
  },
  "embedding": {
    "model": "text-embedding-3-small",
    "dimensions": 1536
  },
  "filterable_fields": {
    "category": {
      "type": "keyword",
      "values": ["api-docs", "tutorials", "faqs", "changelogs"]
    }
  }
}
